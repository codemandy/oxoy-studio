{
  "name": "OXOY-BOILERPLATE",
  "version": "1.0.4",
  "description": "Boilerplate. 11ty, Tailwind, Image optimization, Netlify CMS ",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i _includes/assets/css/tailwind.css -o _includes/assets/css/tailwind.css --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss -i _includes/assets/css/tailwind.css -o _includes/assets/css/tailwind.css --postcss",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate"
  },
  "author": {
    "name": "Dan Urbanowicz",
    "email": "dan.urbanowicz@gmail.com",
    "url": "https://github.com/danurbanowicz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate/issues"
  },
  "homepage": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "autoprefixer": "^10.4.14",
    "clean-css": "^5.3.2",
    "html-minifier": "^4.0.0",
    "luxon": "^3.2.1",
    "markdown-it-anchor": "^8.6.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "^0.11.0"
  }
}
